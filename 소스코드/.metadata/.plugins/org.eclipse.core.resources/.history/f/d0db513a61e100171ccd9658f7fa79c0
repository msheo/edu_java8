			Path path = Paths.get("asset/file" + i + ".txt");
			
			int coreNum = Runtime.getRuntime().availableProcessors();
			Executors.newFixedThreadPool(nThreads)
			
			FileChannel.open(path, options);
			
		}
		
	}
	
}
